@using Kulala.Learning.Common.Model;
@{
    ViewBag.Title = "Module";
}

<h2>Module</h2>
<h3>@ViewBag.Modules</h3>
<table class="table">
    <tr>
        <th>No.</th>

        <th>名称</th>

        <th>详细类型名称</th>
    </tr>
    @foreach (SysEvent item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.TypeName
            </td>
        </tr>
    }
</table>

